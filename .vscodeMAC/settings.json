{
    "files.associations": {
        "*.x": "c",
        "__hash_table": "c",
        "__string": "c",
        "algorithm": "c",
        "array": "c",
        "atomic": "c",
        "bitset": "c",
        "cstdio": "c",
        "cstdlib": "c",
        "cstring": "c",
        "cwchar": "c",
        "memory_resource": "c",
        "istream": "c",
        "iterator": "c",
        "memory": "c",
        "ostream": "c",
        "streambuf": "c",
        "string": "c",
        "string_view": "c",
        "type_traits": "c",
        "unordered_map": "c",
        "vector": "c"
    },
    "code-runner.defaultLanguage": "c",
    "code-runner.runInTerminal": true,
    "code-runner.executorMap": {
        "c": "cd $dir && gcc *.c -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    }
}